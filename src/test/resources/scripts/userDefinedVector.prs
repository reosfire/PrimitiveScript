class Vec2 {
    fun init(x, y) {
        self.x = x
        self.y = y
    }

    fun plus(other) {
        return new.Vec2(
            self.x + other.x,
            self.y + other.y
        )
    }
}

fun main(x1, y1, x2, y2) {
    vec1 = new.Vec2(x1, y1)
    vec2 = new.Vec2(x2, y2)
    return vec1 + vec2
}