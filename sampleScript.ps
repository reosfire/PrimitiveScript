fun fibonacciUpTo(n) {
    if (n.less(1)) {
        return void
    }

    var result = new.list(1)

    if (n.equals(1)) {
        return result
    }
    result.add(1)

    var a = 1
    var b = 1
    var fib = 0
    var i = 2

    while (i.less(n)) {
        fib.set(a.plus(b))
        result.add(new.int(fib))
        a.set(b)
        b.set(fib)
        i.increment()
    }
    return result
}

fun main() {
    while (true) {
        this.println("Enter n: ")
        var n = this.int(this.readln())
        var result = this.fibonacciUpTo(n)
        var i = 0
        var resultSize = result.size()
        while (i.less(resultSize)) {
            this.println(result.get(i))
            i.increment()
        }
    }
}
