fun noArgsFunction() {
    this.noArgsFunction()
    this.print(1)
}

fun recursiveLoop(i) {
    if (i.less(0)) {
        return void
    }

    this.println(i, "hello world")

    this.recursiveLoop(i.minus(1))
}

fun needPrint(i) { return i.modulo(4).equals(0) }

fun main(someNumber) {
    this.recursiveLoop(100)
    var i = 0
    while (true) {
        if (this.needPrint(i)) {
            this.println(i)
        }

        if (i.greater(50.plus(50))) {
            break
        }

        i.increment()
    }

    someNumber.set(this.int(this.readln()))
}
